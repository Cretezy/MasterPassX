{
  "name": "masterpassx-web",
  "version": "1.0.1",
  "license": "MIT",
  "repository": "git@github.com:Cretezy/MasterPassX.git",
  "author": "Charles Crete <charles@cretezy.com>",
  "dependencies": {
    "localforage": "^1.5.5",
    "masterpassx-core": "^0.1.4",
    "react": "^16.2.0",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "reactstrap": "^5.0.0-alpha.4",
    "redux": "^3.7.2",
    "redux-persist": "^5.4.0"
  },
  "devDependencies": {
    "node-sass-chokidar": "^0.0.3",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.9.2",
    "react-scripts": "1.0.17"
  },
  "scripts": {
    "start-js": "react-scripts start",
    "build-js": "react-scripts build",
    "start-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "start": "npm-run-all -p start-css start-js",
    "build": "npm-run-all build-css build-js",
    "test": "npm run build-css && react-scripts test --env=jsdom",
    "prettier": "prettier --use-tabs --write \"src/**/*.js\""
  }
}
